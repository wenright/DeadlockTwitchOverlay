      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^4.21.0",
    "browserify": "^17.0.0",
    "tailwindcss": "^3.4.12",
    "webextension-polyfill": "^0.12.0"
